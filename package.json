{
  "name": "recipes",
  "version": "0.0.0",
  "author": "Quinton Woodmansee <qwoodmansee@capital.edu>",
  "description": "First pass at an attempt for a simple recipe/grocery list calculator for personal use",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    "build:electron": "node .electron-vue/build.js && electron-builder",
    "build:electron:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:electron:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:electron:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "build:nativescript": "webpack --config ./src/nativescript-main/nativescript-webpack.config.js --env.tnsAction build",
    "build:android": "npm run build:nativescript -- --env.android",
    "build:ios": "npm run build:nativescript -- --env.ios",
    "dev:electron": "node .electron-vue/dev-runner.js",
    "dev:nativescript": "webpack --config ./src/nativescript-main/nativescript-webpack.config.js --watch --env.tnsAction run",
    "dev:android": "npm run dev:nativescript -- --env.android",
    "dev:ios": "npm run dev:nativescript -- --env.ios",
    "pack:electron": "npm run pack:main && npm run pack:renderer",
    "pack:electron:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:electron:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "clean": "rimraf dist",
    "postinstall": "npm run lint:fix"
  },
  "build": {
    "productName": "recipes",
    "appId": "org.simulatedgreg.electron-vue",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "vue": "^2.3.3",
    "axios": "^0.16.1",
    "vue-electron": "^1.0.6",
    "nativescript-cardview": "^3.1.1",
    "nativescript-theme-core": "^1.0.4",
    "nativescript-vue": "^1.3.1",
    "tns-core-modules": "~3.4.1",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.7.5",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-builder": "^19.19.1",
    "babel-eslint": "^7.2.3",
    "eslint": "^4.4.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^2.30.1",
    "multispinner": "^0.2.1",
    "nativescript-vue-externals": "^0.2.0",
    "nativescript-vue-loader": "^0.1.5",
    "nativescript-vue-target": "^0.1.0",
    "nativescript-vue-template-compiler": "^1.3.1",
    "node-loader": "^0.6.0",
    "ns-vue-loader": "^0.1.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-synchronizable-shell-plugin": "0.0.7",
    "winston-color": "^1.0.0"
  }
}
